// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://admin:postgrespw@localhost:5432/devdb?schema=public"
}

model Log {
  id            Int    @id @default(autoincrement())
  action        String
  user          String
  stock         String
  amount        Float?
  shareprice    Float?
  shares_bought Int?
}
